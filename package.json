{
  "name": "react-maze",
  "version": "1.0.0",
  "description": "react maze",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wh00527/react-maze.git"
  },
  "keywords": [
    "react",
    "maze",
    "lodash"
  ],
  "author": "Kevin Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wh00527/react-maze/issues"
  },
  "homepage": "https://github.com/wh00527/react-maze#readme",
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.42",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.2.0",
    "jest": "23.6.0"
  },
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover"
    ]
  }
}
